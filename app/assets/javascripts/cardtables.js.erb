     create_card: function(card_id,visible,element_id,position){
	 var card = {
	     card_id: card_id,
	     visible: visible,
	     position: position,
	     element_id: element_id,
	     make_div: function() {
		 if (visible === true ) {
		     return "<div class='card' id='" + element_id + "'>" + CP.divs[card_id-1] +  "</div>";
		 } else {
		     return "<div class='card' id='" + element_id + "'>" + CP.divs[52] +  "</div>";
		 };
	     },
	     get_div: function() {
		 return $('#' + element_id);
		 },
	     show: function(parent,z){
		 console.log(this.make_div());
		 parent.prepend(this.make_div());
		 this.get_div().css({position: "absolute"}).css({left:position.x + "px",tip:position.y+"px",zIndex:z})
	     }
	 }
	  return card;
     }
};

